{
    "author": "mrodrig",
    "name": "json-2-csv",
    "description": "A JSON to CSV and CSV to JSON converter that natively supports sub-documents and auto-generates the CSV heading.",
    "version": "3.2.0",
    "repository": {
        "type": "git",
        "url": "http://github.com/mrodrig/json-2-csv.git"
    },
    "bin": {
        "json2csv": "./bin/json2csv.js",
        "csv2json": "./bin/csv2json.js"
    },
    "main": "./src/converter.js",
    "scripts": {
        "test": "./node_modules/.bin/mocha test/tests.js",
        "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec",
        "lint": "./node_modules/.bin/eslint src bin test"
    },
    "keywords": [
        "json",
        "csv",
        "converter",
        "json2csv",
        "csv2json",
        "json2csv-converter",
        "csv2json-converter",
        "json-2-csv",
        "csv-2-json"
    ],
    "dependencies": {
        "deeks": "2.2.0",
        "doc-path": "2.0.0",
        "underscore": "1.9.1"
    },
    "devDependencies": {
        "commander": "2.19.0",
        "eslint": "5.11.1",
        "istanbul": "0.4.5",
        "mocha": "5.2.0",
        "should": "13.2.3"
    },
    "engines": {
        "node": ">= 6"
    },
    "license": "MIT"
}

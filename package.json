    "dependencies": {
        "deeks": "2.2.5",
        "doc-path": "2.0.3"
    },
    "devDependencies": {
        "babel-eslint": "10.1.0",
        "eslint": "6.8.0",
        "mocha": "7.1.2",
        "nyc": "15.0.1",
        "should": "13.2.3",
        "tslint": "6.1.2",
        "typescript": "3.8.3"
